{
  "name": "camo-tag",
  "version": "0.1.2",
  "description": "Special tags that can freely render only certain tags while leaving child elements intact.",
  "scripts": {
    "bootstrap": "pnpm install",
    "build": "turbo run build",
    "build:packages": "turbo run build --filter @camo-tag/*",
    "test": "turbo run test",
    "lint": "eslint './packages/**/*.{js,ts,jsx,tsx}' --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix './packages/**/*.{js,ts,jsx,tsx}' --report-unused-disable-directives --max-warnings 0",
    "lint:interactive": "eslint-interactive './packages/**/*.{js,ts,jsx,tsx}'",
    "format": "prettier --check './packages/**/*.{js,ts,jsx,tsx}'",
    "format:fix": "prettier --write './packages/**/*.{js,ts,jsx,tsx}'"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-interactive": "^10.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^3.0.3",
    "tsup": "^6.7.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4",
    "vitest": "^0.31.4"
  },
  "engines": {
    "pnpm": ">=8"
  },
  "keywords": [
    "vue",
    "React",
    "Html",
    "Element",
    "front-end"
  ],
  "author": "",
  "license": "MIT"
}
